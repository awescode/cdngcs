{
    "name": "ta-os/cdngcs",
    "description": "Google cloud storage image resizer",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "authors": [{
        "name": "theAlex"
    }],
    "require": {
        "php": "^7.1.3",
        "illuminate/cache": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Awescrm\\CdnGCS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awescrm\\CdnGCS\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awescrm\\CdnGCS\\CdnGCSServiceProvider"
            ],
            "aliases": {
                "CdnGCS": "Awescrm\\CdnGCS\\Facades\\CdnGCS"
            }
        }
    }
}
